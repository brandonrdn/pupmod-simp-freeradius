listen {
  type = <%= @listen_type %>
<% if @ipaddr.include?(':') then -%>
  ipv6addr = <%= @ipaddr %>
<% else -%>
  ipaddr = <%= @ipaddr %>
<% end -%>
  port = <%= @port.to_i %>
<% if not @interface.eql?('nil') then -%>
  interface = <%= @interface %>
<% end -%>
<% if not @per_socket_clients.eql?('') then -%>
  clients = <%= @per_socket_clients %>
<% end -%>
}
